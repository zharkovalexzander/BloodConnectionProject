                        app:srcCompat="@drawable/loader" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/leuData"
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:layout_gravity="top|start"
                    android:layout_marginTop="15dp"
                    android:background="@color/common_signin_btn_dark_text_default"
                    android:elevation="3dp">

                    <TextView
                        android:id="@+id/leuDate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top|center"
                        android:layout_marginLeft="55dp"
                        android:layout_marginTop="10dp"
                        android:ems="10"
                        android:inputType="textPersonName"
                        android:textAlignment="center"
                        android:textColor="@color/material_drawer_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/leuTime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top|center"
                        android:layout_marginLeft="55dp"
                        android:layout_marginTop="30dp"
                        android:ems="10"
                        android:inputType="textPersonName"
