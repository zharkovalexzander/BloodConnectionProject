                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="10dp"
                        android:ems="10"
                        android:inputType="textPersonName"
                        android:text="Erythrocytes"
                        android:textAlignment="textStart"
                        android:textColor="@color/material_drawer_primary_text"
